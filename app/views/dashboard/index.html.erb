<% if(logged_in?) %>
  <%= react_component('Dashboard', {user: current_user,
    projects: @projects,
    portfolio: current_user.portfolio,
    avatar: @user.avatar,
    project_avatars: @project_avatars,
    markdownInfo: @user.markdown_info},
  {prerender: true}) %>
<% else %>
  <%= react_component('Main', {user: current_user,
    signed_in: 0},
  {prerender: true}) %>
<% end %>
